modules = ["web", "nodejs-20", "vue-node-20", "php-8.2"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Vue Ticket App"

[[workflows.workflow]]
name = "Vue Ticket App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd vue-app && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 37513
externalPort = 3000
